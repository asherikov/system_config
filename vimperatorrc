set gui=none,tabs
set complete=
set defsearch=
set focuscontent=true
set history=5

map S :open google 
map T :open http://thesaurus.com/browse/
map D :open http://dictionary.reference.com/browse/

map <C-d> :downloads<CR>

map <C-Left>    :tabprev<CR>
map <C-Right>   :tabnext<CR>


map <F1> :open http://example.com<CR>
