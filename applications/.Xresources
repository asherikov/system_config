!xterm.eightBitInput:    true
!xterm.eightBitOutput:   true
xterm*Background: darkblue
xterm*Foreground: white
!xterm*font: -cronyx-fixed-*-*-*-*-20-*-*-*-*-*-*
xterm*font: -*-*-*-*-*-*-20-*-*-*-*-iso10646-1
xterm*saveLines: 16384
xterm*sameName: false
xterm*visualBell: true
!xterm*deleteIsDEL: true

xterm*termName: xterm-color
xterm*charClass: ""

!workaround for halfpage scrolling glitch
*VT100.translations: #override \n\
      Shift <KeyPress> Prior : scroll-back(20) \n\
      Shift <KeyPress> Next  : scroll-forw(20) \n
